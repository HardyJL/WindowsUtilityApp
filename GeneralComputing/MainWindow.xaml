<Window x:Class="GeneralComputing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeneralComputing"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,405,0,0" VerticalAlignment="Top" Height="19" Width="206" Command="{Binding GeneratePasswordCommand}" />
        <TextBlock Text="{Binding PasswordText}" HorizontalAlignment="Center" Margin="0,121,0,0" VerticalAlignment="Top" Height="158" Width="738" RenderTransformOrigin="0.489,0.507" TextWrapping="Wrap" />
    </Grid>
</Window>